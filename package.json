{
  "name": "angular-svg-icons",
  "version": "0.0.4",
  "description": "Angular component to display icons from SVG sprites",
  "license": "MIT",
  "author": "Bastien Moulia",
  "repository": {
    "url": "git+https://github.com/bastienmoulia/angular-svg-icons.git"
  },
  "keywords": [
    "Angular", "SVG", "icon", "glyph"
  ],
  "dependencies": {
    "angular": "^1.6.0"
  },
  "devDependencies": {
    "@types/angular": "^1.5.22",
    "http-server": "^0.9.0",
    "node-sass": "^4.1.1",
    "svg-sprite": "^1.3.6",
    "typescript": "^2.1.4",
    "uglify-js": "^2.7.5"
  },
  "scripts": {
    "start": "http-server ./ -c1",
    "build": "npm run tsc && npm run sass",
    "svg-sprite": "svg-sprite -s --dest=demo demo/svg/*.svg",
    "tsc": "tsc",
    "sass": "node-sass src/angular-svg-icons.scss dist/angular-svg-icons.css"
  }
}
